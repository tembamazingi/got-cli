######################### LARAVEL VALET #########################
alias v*='valet link'
alias v*!='valet unlink'
alias v!='valet restart'
alias v:o='valet open'
alias v:s='valet secure'
alias v:php='valet log php-fpm -f --lines=5'
alias v:ngx='valet log nginx -f --lines=5'

######################### WP-CLI #########################
alias w:p='php -d memory_limit=2048M "$(which wp)" '
